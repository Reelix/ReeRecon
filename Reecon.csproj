<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
	<TargetFramework>net6.0</TargetFramework>
	<PublishSingleFile Condition="'$(Configuration)' == 'Release'">true</PublishSingleFile>
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
	<PlatformTarget>x64</PlatformTarget>
	<StartupObject>Reecon.Program</StartupObject>
	<SignAssembly>false</SignAssembly>
	<SelfContained>false</SelfContained>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FluentFTP" Version="38.0.0" />
    <PackageReference Include="LdapForNet" Version="2.7.15" />
    <PackageReference Include="MySqlConnector" Version="2.1.11" />
    <PackageReference Include="Npgsql" Version="6.0.5" />
	<EmbeddedResource Include="Ports.txt" />
  </ItemGroup>

</Project>  